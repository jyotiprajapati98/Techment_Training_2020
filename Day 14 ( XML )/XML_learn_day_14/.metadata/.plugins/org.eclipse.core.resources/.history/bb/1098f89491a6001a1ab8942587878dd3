/*
 * print this pattern 
 * 
1 2 3 4 5
1 2 3 4
1 2 3
1 2
1
*/
package com.tech.problemStatements;

public class PrintPatternEx11 {

	public static void main(String[] args) {
		int rows=5;
		
		for(int i=0;i<=rows;i++ ) {
			
			for(int j=1;j<=rows;j++) {
				
				System.out.print(" "+i);
			}
			System.out.println(" ");
		}

	}

}
